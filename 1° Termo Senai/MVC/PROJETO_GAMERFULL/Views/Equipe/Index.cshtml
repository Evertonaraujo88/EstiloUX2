@model Equipe

@{
    ViewData["Title"] = "Equipe";
}

<h1>Tela de Equipes</h1>

    <!-- 
    post -> enviar
    get -> obter 
    -->
<form method="post" action="@Url.Action("Cadastar") enctype= "multipart/form-data">
    <label for="Nome">Nome</label>
    <input type="text" name="Nome" id="">

    <label for="Imagem">Imagem</label>
    <input type="file" name="Imagem" id="">

    <button type = "submit">Cadastrar</button>
</form>

<table class= "table table-striped table-responsive">
    <thead>
        <th>Id</th>
        <th>Nome</th>
        <th>Imagem</th>
    </thead>

    
    <tbody>

        @foreach (Equipe equipe in ViewBag.Equipe)
        {
             <tr>
                <td>@equipe.IdEquipe</td>
                <td>@equipe.Nome</td>
                <td>@equipe.Imagem</td>
            </tr>
        }

       

    </tbody>
</table>